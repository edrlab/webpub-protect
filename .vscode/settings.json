{
    "scss.validate": false,
    "css.validate": false,
    "less.validate": false,
    "search.exclude": {
        ".history": true,
        "node_modules": true,
        "package-lock.json": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
